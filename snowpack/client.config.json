{
  "install": [
    "svelte"
  ],
  "plugins": [
    ["@snowpack/plugin-svelte", {
      "generate": "dom",
      "hydratable": true
    }]
  ],
  "devOptions": {
    "port": 3002,
    "out": "build/client",
    "open": "none"
  },
  "mount": {
    "routes": "/_routes/",
    "sapper": "/_sapper/",
    "shared": "/shared/",
    "public": "/"
  },
  "alias": {
    "@shared": "./shared"
  }
}
